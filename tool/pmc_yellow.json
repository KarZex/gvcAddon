                    {
                        "filters": {
                            "all_of": [
                                {
                                    "test": "is_family",
                                    "subject": "other",
                                    "value": "player"
                                },
                                {
                                    "test": "is_family",
                                    "subject": "other",
                                    "operator": "!=",
                                    "value": "yellowteam"
                                },
                                {
                                    "test": "has_tag",
                                    "operator": "!=",
                                    "subject": "other",
                                    "value": "yellowSub"
