            "minecraft:health": {
                "value": 20,
                "max": 20
            },
            "minecraft:hurt_on_condition": {
                "damage_conditions": [
                    {
                        "filters": {
                            "test": "in_lava",
                            "subject": "self",
                            "operator": "==",
                            "value": true
                        },
                        "cause": "lava",
                        "damage_per_tick": 4
                    }
                ]
            },
            "minecraft:interact": {
                "interactions": [
                    {
                        "use_item": false,
                        "play_sounds": "enderchest.open",
                        "interact_text": "action.gvc.item",
                        "on_interact": {
                            "filters": {
                                "all_of": [
                                    {
                                        "test": "is_sneaking",
                                        "subject": "other",
                                        "value": false
                                    },
                                    {
                                        "test": "is_family",
                                        "subject": "other",
                                        "value": "yellowteam"
                                    }
                                ]
                            },
                            "event": "gvcv5:order",
