                                "any_of": [
                                    {
                                        "all_of": [
                                            {
                                                "test": "is_family",
                                                "subject": "other",
                                                "value": "allied_soldier"
                                            },
                                            {
                                                "test": "is_family",
                                                "subject": "other",
                                                "value": "yellowteam"
                                            }
                                        ]
                                    },
                                    {
                                        "all_of": [
                                            {
                                                "test": "is_family",
                                                "subject": "other",
                                                "value": "player"
                                            },
                                            {
                                                "test": "has_tag",
                                                "operator": "==",
                                                "subject": "other",
                                                "value": "yellowSub"
                                            }
                                        ]
                                    }
                                ]
                            }
                        }
                    }
                ]
            },
