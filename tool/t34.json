{
  "format_version": "1.18.10",
  "minecraft:entity": {
    "description": {
      "identifier": "vehicle:t34",
      "is_spawnable": true,
      "is_summonable": true,
      "is_experimental": false,
      "scripts": {
	    "animate": [ "walk","vtext" ]
	  },
	  "animations": {
	    "walk": "controller.animation.vehicle",
	    "vtext": "controller.animation.vtext"
	  },
      "properties": {
        "zex:tank": {
            "client_sync": true,
            "type": "int",
            "range": [
                0,
                255
            ],
            "default": 0
        }
      }
    },
    "component_groups": {
			"summon_enemy": {
        "minecraft:despawn": {
          "despawn_from_distance": {}
        },
        "minecraft:loot": {
          "table": "loot_tables/entities/t55_enemy.json"
        },
        "minecraft:environment_sensor": {
          "triggers": [
            {
              "event": "despawning",
              "target": "self",
              "filters": { "test": "has_tag", "subject": "self", "operator": "not", "value": "has.rider" }
            }
          ]
        }
			},
			"despawning": {
        "minecraft:instant_despawn": {}
			},
      "gvcv5:vehicle_lock": {
        "minecraft:rideable": {
          "seat_count": 1,
          "family_types": [
            "player"
          ],
          "interact_text": "action.interact.ride.horse",
          "seats": {
            "position": [
              0,
              0.5,
              -0.4
            ]
          }
        }
      }
    },

    "components": {
        "minecraft:boss": {
          "name": "entity.vehicle:t34.name",
          "should_darken_sky": false,
          "hud_range": 4
        },
        "minecraft:experience_reward": {
          "on_death": "query.last_hit_by_player ? Math.Random(1,3) : 0"
        },
        "minecraft:loot": {
          "table": "loot_tables/entities/tank.json"
        },
        "minecraft:can_power_jump":{
		},
		"minecraft:input_ground_controlled": {
		},
		"minecraft:horse.jump_strength": {
			"value": {
				"range_min": 0,
				"range_max": 0
			}
		},
        "minecraft:rideable": {
          "seat_count": 1,
          "family_types": [
            "player","germany"
          ],
          "interact_text": "action.interact.ride.horse",
          "seats": {
            "position": [ 0.0, 1.7, -0.1 ]
          }
        },
      "minecraft:interact": {
        "interactions": [
			{
				"hurt_item": 1,
				"play_sounds": "irongolem.repair",
				"interact_text": "action.interact.repair",
				"health_amount": 10,
				"on_interact": {
					"filters": {
						"all_of": [
						{ "test": "is_family","subject": "other","value": "player"},
						{ "test": "has_equipment", "subject": "other", "domain": "hand", "value": "zex:rep"},
						{ "test": "is_missing_health" }
						]
					}
				}
			},
			{
				"hurt_item": 1,
				"spawn_items": {
					"table": "loot_tables/entities/t34.json"
				},
				"play_sounds": "enderchest.open",
				"interact_text": "action.gvc.item",
				"on_interact": {
					"filters": {
						"all_of": [
						{ "test": "is_family","subject": "other","value": "player"},
						{ "test": "has_equipment", "subject": "other", "domain": "hand", "value": "zex:rep"},
						{ "test": "has_tag", "subject": "self", "operator": "not", "value": "has.rider" },
						{ "test": "is_missing_health","operator": "not" }
						]
					},
					"event": "despawning",
					"target": "self"
				}
			},
          {
            "interact_text": "action.gvc.lock",
            "on_interact": {
              "filters": {
                "all_of": [
                  {
                    "test": "is_family",
                    "subject": "other",
                    "value": "player"
                  },
                  {
                    "test": "has_tag",
                    "subject": "self",
                    "value": "has.rider"
                  }
                ]
              },
              "event": "vehicle_lock",
              "target": "self"
            }
          }
        ]
      },
      "minecraft:type_family": {
        "family": [ "vehicle","playerfire","plate","tank" ]
      },
      "minecraft:fire_immune": {
      },
      "minecraft:breathable": {
        "total_supply": 35,
        "suffocate_time": 0,
        "breathes_air": true,
        "breathes_water": true
      },
      "minecraft:health": {
        "value": 160,
        "max": 160
      },
      "minecraft:scale": {
        "value": 1.0
      },
      "minecraft:movement": {
        "value": 0.2
      },
	   "minecraft:knockback_resistance": {
        "value": 1.0
      },
      "minecraft:navigation.walk": {
        "is_amphibious": true,
        "avoid_water": true
      },
      "minecraft:movement.basic": {
      },
      "minecraft:jump.static": {
      },
      "minecraft:nameable": {
      },
      "minecraft:behavior.player_ride_tamed": {
      },
      "minecraft:balloonable": {
      },
      "minecraft:collision_box": {
        "width": 3,
        "height": 1
      },
      "minecraft:damage_sensor": {
        "triggers": [
          {
            "cause": "projectile",
            "deals_damage": false,
            "on_damage_sound_event": "non_projectile"
          },
          {
            "cause": "fall",
            "deals_damage": false
          }
	   ]
      },                
      "minecraft:is_tamed": {
      },
      "minecraft:pushable": {
        "is_pushable": true,
        "is_pushable_by_piston": false
      },
      "minecraft:physics": {
      }
    },
    "events": {
      "summon_enemy": {
        "add": {
          "component_groups": [
            "summon_enemy"
          ]
        },
        "queue_command": {
          "command":[
            "tag @s add is_enemy"
          ]
        }
      },
      "despawning": {
        "add": {
          "component_groups": [
            "despawning"
          ]
        }
      },
      "vehicle_lock": {
        "remove": {
          "component_groups": [
            "summon_enemy"
          ]
        },
        "add": {
          "component_groups": [
            "gvcv5:vehicle_lock"
          ]
        },
        "queue_command": {
          "command": [
            "tellraw @s {\"rawtext\":[{\"text\":\"Vehicle Lock\"}]}",
            "ride @e[c=1,family=vehicle] evict_riders"
          ],
          "target": "other"
        }
      }
    }
  }
}