{
  "format_version": "1.21.0",
  "minecraft:entity": {
    "description": {
      "identifier": "minecraft:player",
      "spawn_category": "creature",
      "is_spawnable": false,
      "is_summonable": false,
      "scripts": {
        "pre_animation": [
          "variable.cooltime = 0.0;"
        ],
        "animate": [
          "guns",
          "hold",
          "para",
          "heri",
          "ads"
        ]
      },
      "animations": {
        "guns": "controller.animation.guns",
        "hold": "controller.animation.hold",
        "para": "controller.animation.para",
        "ads": "controller.animation.ads",
        "heri": "controller.animation.heri"
      }
    },

    "component_groups": {
      "minecraft:add_raid_omen": {
        "minecraft:spell_effects": {
          "add_effects": [
            {
              "effect": "raid_omen",
              "duration": 30,
              "display_on_screen_animation": true
            }
          ],
          "remove_effects": "bad_omen"
        },
        "minecraft:timer": {
          "time": [ 0.0, 0.0 ],
          "looping": false,
          "time_down_event": {
            "event": "minecraft:clear_add_raid_omen",
            "target": "self"
          }
        }
      },
      "minecraft:clear_raid_omen_spell_effect": {
        "minecraft:spell_effects": {
        }
      },
      "minecraft:raid_trigger": {
        "minecraft:raid_trigger": {
          "triggered_event": {
            "event": "minecraft:remove_raid_trigger",
            "target": "self"
          }
        }
      },
      "gvcv5:in_down": {
        "minecraft:type_family": {
          "family": [ "down" ]
        },
        "minecraft:damage_sensor": {
          "triggers": [
            {
              "cause": "entity_attack",
              "deals_damage": false,
              "on_damage": {
                    "filters": {
                      "all_of": [
                        {
                          "test": "has_tag",
                          "subject":"self",
                          "operator": "==",
                          "value": "down"
                        },
                        {
                          "test": "is_family",
                          "subject": "other",
                          "value": "whiteteam"
                        }
                    ]
                  },
                  "event": "gvcv5:become_rise",
                  "target": "self"
               }
            },
            {
              "cause": "all",
              "deals_damage": false,
              "on_damage": {
                    "filters": {
                      "any_of": [
                        {
                          "test": "is_family",
                          "subject": "other",
                          "operator": "!=",
                          "value": "player"
                        },
                        {
                          "test": "is_family",
                          "subject": "other",
                          "operator": "==",
                          "value": "whiteteam"
                        }
                    ]
                  }
               }
            },
            {
              "cause": "all",
              "deals_damage": true,
              "on_damage": {
                    "filters": {
                      "all_of": [
                        {
                          "test": "has_tag",
                          "subject":"self",
                          "operator": "==",
                          "value": "down"
                        },
                        {
                          "test": "has_damage",
                          "value": "fatal"
                        }
                    ]
                  },
                  "event": "gvcv5:remove_down",
                  "target": "self"
               }
            }
          ]
        }
      },
      "gvcv5:team": {
        "minecraft:type_family": {
          "family": [ "player","playerp","mod","whiteteam","red" ]
        },
        "minecraft:damage_sensor": {
          "triggers": [
            {
              "cause": "all",
              "deals_damage": false,
              "on_damage": {
                    "filters": {
                      "any_of": [
                        {
                          "test": "is_family",
                          "subject": "other",
                          "value": "noteam"
                        },
                        {
                          "test": "is_family",
                          "subject": "other",
                          "value": "whiteteam"
                        }
                    ]
                  }
               }
            },
            {
              "cause": "all",
              "deals_damage": false,
              "on_damage": {
                    "filters": {
                      "all_of": [
                        {
                          "test": "has_tag",
                          "subject":"self",
                          "operator": "!=",
                          "value": "down"
                        },
                        {
                          "test": "is_riding",
                          "value": false
                        },
                        {
                          "test": "has_damage",
                          "value": "fatal"
                        }
                    ]
                  },
                  "event": "gvcv5:become_down",
                  "target": "self"
               }
            }
          ]
        }
      },
      "fire:fire": {
        "minecraft:spawn_entity": {
          "entities": [
            {
              "max_wait_time": 0,
              "min_wait_time": 0,
              "num_to_spawn": 1,
              "single_use": true,
              "spawn_entity": "fire:fire"
            }
          ]
        }
      }
    },

    "components": {
      "minecraft:experience_reward": {
        "on_death": "Math.Min(query.player_level * 7, 100)"
      },
      "minecraft:type_family": {
        "family": [ "player","playerp","mod","noteam" ]
      },
      "minecraft:is_hidden_when_invisible": {
      },
      "minecraft:loot": {
        "table": "loot_tables/empty.json"
      },
      "minecraft:collision_box": {
        "width": 0.6,
        "height": 1.8
      },

      "minecraft:damage_sensor": {
        "triggers": [
          {
            "cause": "all",
            "deals_damage": false,
            "on_damage": {
                  "filters": {
                      "all_of": [
                        {
                          "test": "is_family",
                          "subject": "other",
                          "value": "allied_soldier"
                        }
                      ]
                  }
                }
          }
        ]
      },
      "minecraft:can_climb": {
      },
      "minecraft:movement": {
        "value": 0.1
      },
      "minecraft:hurt_on_condition": {
        "damage_conditions": [
          {
            "filters": {
              "test": "in_lava",
              "subject": "self",
              "operator": "==",
              "value": true
            },
            "cause": "lava",
            "damage_per_tick": 4
          }
        ]
      },
      "minecraft:attack": {
        "damage": 1
      },
      "minecraft:exhaustion_values": {
        "heal": 6,
        "jump": 0.05,
        "sprint_jump": 0.2,
        "mine": 0.005,
        "attack": 0.1,
        "damage": 0.1,
        "walk": 0.0,
        "sprint": 0.1,
        "swim": 0.01
      },
      "minecraft:player.saturation": {
        "value": 5,
        "max": 20
      },
      "minecraft:player.exhaustion": {
        "value": 0,
        "max": 20
      },
      "minecraft:player.level": {
        "value": 0,
        "max": 24791
      },
      "minecraft:player.experience": {
        "value": 0,
        "max": 1
      },
      "minecraft:breathable": {
        "total_supply": 15,
        "suffocate_time": -1,
        "inhale_time": 3.75,
        "generates_bubbles": false
      },
      "minecraft:nameable": {
        "always_show": true,
        "allow_name_tag_renaming": false
      },
      "minecraft:physics": {
        "push_towards_closest_space": true
      },
      "minecraft:pushable": {
        "is_pushable": false,
        "is_pushable_by_piston": true
      },
      "minecraft:insomnia": {
        "days_until_insomnia": 3
      },
      "minecraft:rideable": {
        "seat_count": 2,
        "family_types": [
          "parrot_tame"
        ],
        "pull_in_entities": true,
        "seats": [
          {
            "position": [ 0.4, -0.2, -0.1 ],
            "min_rider_count": 0,
            "max_rider_count": 0,
            "lock_rider_rotation": 0
          },
          {
            "position": [ -0.4, -0.2, -0.1 ],
            "min_rider_count": 1,
            "max_rider_count": 2,
            "lock_rider_rotation": 0
          }
        ]
      },
      "minecraft:conditional_bandwidth_optimization": {
      },
      "minecraft:block_climber": {},
      "minecraft:environment_sensor": {
        "triggers": {
          "filters": {
            "all_of": [
              {
                "test": "has_mob_effect",
                "subject": "self",
                "value": "bad_omen"
              },
              {
                "test": "is_in_village",
                "subject": "self",
                "value": true
              }
            ]
          },
          "event": "minecraft:gain_raid_omen"
        }
      }
    },

    "events": {
      "minecraft:gain_raid_omen": {
        "add": {
          "component_groups": [
            "minecraft:add_raid_omen"
          ]
        }
      },
      "minecraft:clear_add_raid_omen": {
        "remove": {
          "component_groups": [
            "minecraft:add_raid_omen"
          ]
        },
        "add": {
          "component_groups": [
            "minecraft:clear_raid_omen_spell_effect"
          ]
        }
      },
      "minecraft:trigger_raid": {
        "add": {
          "component_groups": [ "minecraft:raid_trigger" ]
        }
      },
      "minecraft:remove_raid_trigger": {
        "remove": {
          "component_groups": [ "minecraft:raid_trigger" ]
        }
      },
      "fire:fire": {
        "add": {
          "component_groups": [
            "fire:fire"
          ]
        }
      },
      "gvcv5:become_team": {
        "add": {
          "component_groups": [
            "gvcv5:team"
          ]
        }
      },
      "gvcv5:become_down": {
        "queue_command":{
          "command":[
            "effect @s instant_health 1 255 true",
            "effect @s resistance 4 255 true",
            "effect @s wither 99999 1 true",
            "tag @s add down"
          ]
        },
        "add": {
          "component_groups": [
            "gvcv5:in_down"
          ]
        }
      },
      "gvcv5:remove_down": {
        "queue_command":{
          "command":[
            "tag @s remove down"
          ]
        },
        "remove": {
          "component_groups": [
            "gvcv5:in_down"
          ]
        },
        "add": {
          "component_groups": [
            "gvcv5:team"
          ]
        }
      },
      "gvcv5:become_rise": {
        "sequence": [
          {
            "queue_command":{
              "command":[
                "playanimation @s animation.down.main2 none 6 ",
                "tag @s add rise",
                "effect @s resistance 7 255 true",
                "effect @s wither 0",
                "scoreboard players set @s rise 120"
              ],
              "target":"self"
            }
        },
        {
          "queue_command":{
            "command":[
              "tp @s @p[tag=down]",
              "playanimation @s animation.down.sub2 none 6 ",
              "tag @s add rise",
              "scoreboard players set @s rise 120"
            ],
            "target":"other"
          }
      }
	   ]
    }
      }
    }
  }




