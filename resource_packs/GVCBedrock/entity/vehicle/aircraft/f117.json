{
  "format_version": "1.10.0",
  "minecraft:client_entity": {
    "description": {
      "identifier": "vehicle:f117",
      "materials": { "default": "entity_alphablend" },
      "textures": {
        "default": "textures/entity/f117"
      },
      "geometry": {
        "default": "geometry.f117"
      },
			"animations": {
				"fly": "animation.airmig172",
				"flying": "animation.flying"
			},
			"scripts": {
				"pre_animation": [
					"v.rotation = q.body_y_rotation - q.rider_head_y_rotation(0);",
					"v.applyRotation = v.rotation;",
					"(v.rotation > 180) ? { v.applyRotation = v.rotation - 360; };",
					"(v.rotation < -180) ? { v.applyRotation = v.rotation + 360; };"
				],
				"animate": [
					{
						"flying": "!query.is_on_ground"
					},
					{
						"fly": "query.has_rider && query.is_moving"
					}
				]
			},
	  "spawn_egg":{
	  "texture":"f117",
	  "texture_index":0
	  },
      "render_controllers": [ "controller.render.iron_golem" ]
    }
  }
}