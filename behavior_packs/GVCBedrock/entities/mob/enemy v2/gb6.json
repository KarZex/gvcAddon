{
  "format_version": "1.21.0",
  "minecraft:entity": {
    "description": {
      "identifier": "gvcv5:gb6",
      "spawn_category": "monster",
      "is_spawnable": true,
      "is_summonable": true
    },
    "component_groups": {
      "is_raid":{
        "minecraft:damage_sensor": {
          "triggers": [
            {
              "cause": "fall",
              "deals_damage": false
            }
          ]
        },
        "minecraft:behavior.nearest_attackable_target": {
          "priority": 2,
          "must_see": false,
          "reselect_targets": true,
          "scan_interval": 10,
          "entity_types": [
            {
              "filters": {
                "test": "is_family",
                "subject": "other",
                "value": "player"
              },
              "max_dist": 16
            },
            {
              "filters": {
                "test": "is_family",
                "subject": "other",
                "value": "allied_soldier"
              },
              "max_dist": 16
            },
            {
              "filters": {
                "test": "is_family",
                "subject": "other",
                "value": "villager"
              },
              "max_dist": 16
            },
            {
              "filters": {
                "test": "is_family",
                "subject": "other",
                "value": "wandering_trader"
              },
              "max_dist": 16
            },
            {
              "filters": {
                "test": "is_family",
                "subject": "other",
                "value": "allied_soldier_flag"
              },
              "max_dist": 160
            }
          ]
        },
        "minecraft:follow_range": {
          "value": 256
        },
        "minecraft:floats_in_liquid": {},
        "minecraft:navigation.generic": {
          "is_amphibious": true,
          "can_path_over_water": true,
          "can_break_doors": true,
          "can_swim": true,
          "can_walk": true,
          "avoid_sun": false
        },
        "minecraft:underwater_movement": {
          "value": 0.2
        }

      },
      "can_despawn": {
        "minecraft:despawn": {
          "despawn_from_distance": {}
        }
      },
      "ptrd": {
        "minecraft:behavior.ranged_attack": {
          "priority": 3,
          "burst_shots": 1,
          "burst_interval": 0.1,
          "charge_charged_trigger": 0.0,
          "charge_shoot_trigger": 0.0,
          "attack_interval_min": 3.0,
          "attack_interval_max": 3.0,
          "attack_radius": 120.0
        },
        "minecraft:shooter": {
          "def": "fire:ads_ptrd"
        }
      }
    },

    "components": {
      "minecraft:is_hidden_when_invisible": {
      },
      "minecraft:experience_reward": {
        "on_death": "query.last_hit_by_player ? 20 + (query.equipment_count * Math.Random(1,3)) : 0"
      },
      "minecraft:collision_box": {
        "width": 0.5,
        "height": 1.8
      },
      "minecraft:type_family": {
        "family": [ "guerrilla", "monster", "mob" ]
      },
      "minecraft:breathable": {
        "total_supply": 15,
        "suffocate_time": 0,
        "breathes_water": true
      },

      "minecraft:health": {
        "value": 40,
        "max": 40
      },
      "minecraft:hurt_on_condition": {
        "damage_conditions": [
          {
            "filters": { "test": "in_lava", "subject": "self", "operator": "==", "value": true },
            "cause": "lava",
            "damage_per_tick": 4
          }
        ]
      },
      "minecraft:follow_range": {
        "value": 128,
        "max": 128
      },
      "minecraft:loot": {
        "table": "loot_tables/entities/ga.json"
      },
      "minecraft:movement": {
        "value": 0.25
      },
      "minecraft:navigation.walk": {
        "is_amphibious": false,
        "avoid_sun": false,
        "avoid_water": true
      },
      "minecraft:movement.basic": {
      },
      "minecraft:jump.static": {
      },
      "minecraft:can_climb": {
      },
      "minecraft:nameable": {
      },
      "minecraft:equipment": {
        "table": "loot_tables/entities/gb6.json"
      },
      "minecraft:shooter": {
        "def": "minecraft:arrow"
      },
      "minecraft:behavior.ranged_attack": {
        "priority": 0,
        "attack_interval_min": 1.0,
        "attack_interval_max": 3.0,
        "attack_radius": 15.0
      },
      "minecraft:behavior.hurt_by_target": {
        "priority": 1,
        "entity_types": {
            "filters": {
              "test": "is_family",
              "subject": "other",
              "operator": "!=",
              "value": "guerrilla"
            }
          }
      },
      "minecraft:behavior.nearest_attackable_target": {
        "priority": 2,
        "must_see": true,
        "reselect_targets": true,
        "entity_types": [
          {
            "filters": {
              "test": "is_family",
              "subject": "other",
              "value": "player"
            },
            "max_dist": 120
          },
          {
            "filters": {
              "test": "is_family",
              "subject": "other",
              "value": "allied_soldier"
            },
            "max_dist": 120
          },
          {
            "filters": {
              "test": "is_family",
              "subject": "other",
              "value": "villager"
            },
            "max_dist": 120
          },
          {
            "filters": {
              "test": "is_family",
              "subject": "other",
              "value": "wandering_trader"
            },
            "max_dist": 120
          },
          {
            "filters": {
              "test": "is_family",
              "subject": "other",
              "value": "irongolem"
            },
            "max_dist": 120
          }
        ]
      },
      "minecraft:behavior.equip_item": {
        "priority": 3
      },
      "minecraft:physics": {
      },
      "minecraft:pushable": {
        "is_pushable": true,
        "is_pushable_by_piston": true
      },
      "minecraft:conditional_bandwidth_optimization": {
      }
    },

    "events": {
      "gvcv5:entity_spawned_raid": {
        "sequence": [
          {
          "add": {
            "component_groups": [
              "is_raid"
            ]
          },
          "remove": {
            "component_groups": [
              "can_despawn"
            ]
          }
        },
          {
          "queue_command": {
            "command": [
              "tag @s add raid"
            ]
          }
        },
        {
            "filters": {
              "any_of": [
                {
                  "test": "has_biome_tag",
                  "value": "ocean"
                }
              ]
            },
            "add": { "component_groups": [ "navy" ] }
         }
          ]
      },
      "minecraft:entity_spawned": {
        "add": {
          "component_groups": [
            "can_despawn", "ptrd"
          ]
        }
      },
      "minecraft:spawned_from_block": {
        "add": {
          "component_groups": [
            "ptrd"
          ]
        },
        "remove": {
          "component_groups": [
            "can_despawn"
          ]
        }
      }
    }
  }
}
